{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"clean": {
			"dependsOn": [],
			"cache": false
		},
		"format": {
			"dependsOn": []
		},
		"lint": {
			"dependsOn": ["^build", "^lint"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"test": {
			"dependsOn": ["^build", "^test"],
			"env": []
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": []
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"env": []
		}
	}
}
